/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.box {
  margin-bottom: 5px;
  border-bottom: 1px dashed red;
  padding: 5px 10px;
}

.box-title {
  color: white;
  display: inline-block;
  margin-bottom: 10px;
  background: rgba(0,0,0,0.7);
  padding: 5px 10px;
}
